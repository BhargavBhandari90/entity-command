{
    "name": "wp-cli/entity-command",
    "description": "Manage WordPress core entities.",
    "type": "wp-cli-package",
    "homepage": "https://github.com/wp-cli/entity-command",
    "support": {
        "issues": "https://github.com/wp-cli/entity-command/issues"
    },
    "license": "MIT",
